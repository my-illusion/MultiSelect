{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime", 
        {
          "corejs": 3, // 指定 runtime-corejs 的版本，目前有 2 3 两个版本
          "helpers": true
        }
      ],
      "@babel/plugin-proposal-optional-chaining"
    ]
}